{
  "name": "kaukau",
  "private": false,
  "version": "4.0.0-alpha.0",
  "description": "JS test tools, Mocha wrapper",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/kisiwu/kaukau.git"
  },
  "scripts": {
    "testcmd": "node ./bin/kaukau",
    "cmd": "node ./bin/kaukau --file test/tests --parameters test/config/parameters --config test/config/main.json",
    "init": "node ./bin/kaukau init --file test/tests/o2 --config test/config/main.json",
    "test": "node ./test/test.js",
    "watch": "node ./bin/kaukau --file test/tests --parameters test/config/parameters --config test/config/main.json --watch test/**/*.spec.js test/config/**/*.{json,js} --logs"
  },
  "bin": {
    "kaukau": "bin/kaukau"
  },
  "keywords": [
    "kaukau",
    "mocha",
    "test"
  ],
  "author": "demingongo (https://github.com/demingongo)",
  "license": "MIT",
  "dependencies": {
    "@novice1/logger": "^1.4.0",
    "async-waterfall": "^0.1.5",
    "axios": "^1.7.2",
    "chai": "^4.4.1",
    "chokidar": "^3.6.0",
    "commander": "^12.1.0",
    "deep-property-access": "^1.0.1",
    "mkdirp": "^3.0.1",
    "mocha": "^10.4.0",
    "request-mocha": "^0.2.0"
  },
  "devDependencies": {
    "mochawesome": "^7.1.3"
  }
}
